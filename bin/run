#!/usr/bin/env node

var rl = require("../app");

 rl.add([1, 5], (err) => {
     if(err) {
         console.log("Error: Cannot add range: ", err);
         return false;
     }
 });
 rl.print();
 rl.add([10, 20], (err) => {
     if(err) {
         console.log("Error: Cannot add range: ", err);
         return false;
     }
 });
 rl.print();    // Should display: [1, 5) [10, 20)
 rl.add([20, 20], (err) => {
     if(err) {
         console.log("Error: Cannot add range: ", err);
         return false;
     }
 });
 rl.print();    // Should display: [1, 5) [10, 20)
 rl.add([20, 21], (err) => {
     if(err) {
         console.log("Error: Cannot add range: ", err);
         return false;
     }
 });
 rl.print();    // Should display: [1, 5) [10, 21)
 rl.add([2, 4], (err) => {
     if(err) {
         console.log("Error: Cannot add range: ", err);
         return false;
     }
 });
 rl.print();    // Should display: [1, 5) [10, 21)
 rl.add([3, 8], (err) => {
     if(err) {
         console.log("Error: Cannot add range: ", err);
         return false;
     }
 });
 rl.print();    // Should display: [1, 8) [10, 21)
 rl.remove([10, 10], (err) => {
     if(err) {
         console.log("Error: Cannot remove range: ", err);
         return false;
     }
 });
 rl.print();    // Should display: [1, 8) [10, 21)
 rl.remove([10, 11], (err) => {
     if(err) {
         console.log("Error: Cannot remove range: ", err);
         return false;
     }
 });
 rl.print();    // Should display: [1, 8) [11, 21)
 rl.remove([15, 17], (err) => {
     if(err) {
         console.log("Error: Cannot remove range: ", err);
         return false;
     }
 });
 rl.print();    // Should display: [1, 8) [11, 15) [17, 21)
 rl.remove([3, 19], (err) => {
     if(err) {
         console.log("Error: Cannot remove range: ", err);
         return false;
     }
 });
 rl.print();    // Should display: [1, 3) [19, 21)